#создаем функции для проверки числа на простоту
def isPrime(a):
    k = 0
    for i in range(2, int(a // 2 + 1)):
        if (a % i == 0):
            k = k+1
    if (k <= 0):
        print("Число простое")
    else:
        print("Число не является простым")
n=float(input('введите число: ')) #вводим число с клавиатуры
isPrime(n) #вызываем функцию 
